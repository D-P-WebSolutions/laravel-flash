{
    "name": "dp-websolutions/laravel-flash",
    "require": {
        "php": ">=8.0",
        "illuminate/session": "^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "orchestra/testbench": "^7.0",
        "vimeo/psalm": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Luca",
            "email": "info@lucapuddu.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DPWebSolutions\\LaravelFlash\\": "src/"
        },
        "files": [
            "helpers/flash.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "DPWebSolutions\\LaravelFlash\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DPWebSolutions\\LaravelFlash\\LaravelFlashServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
